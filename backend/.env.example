# Variables de entorno para BACKEND
# Copia este archivo como .env y completa con tus valores reales

# Base de datos MongoDB
MONGODB_URI=mongodb+srv://tu_usuario:tu_password@cluster0.xxxxx.mongodb.net/tu_database

# Secreto para JWT (usa un string aleatorio largo y seguro)
JWT_SECRET=tu_jwt_secret_super_seguro_y_aleatorio

# Puerto del servidor (por defecto 3000)
PORT=3000

# Entorno de ejecución
NODE_ENV=development

# Google OAuth (opcional, solo si usas autenticación con Google)
GOOGLE_CLIENT_ID=tu_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=tu_google_client_secret
